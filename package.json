{
  "name": "portfolio-fullstack",
  "version": "1.0.0",
  "description": "Full-stack portfolio application with React frontend and Express backend",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "build": "npm run frontend:build",
    "start": "npm run frontend:start",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:start": "cd frontend && npm run start",
    "frontend:install": "cd frontend && npm install",
    "backend:dev": "cd backend && npm run dev",
    "backend:start": "cd backend && npm start",
    "backend:seed": "cd backend && npm run seed",
    "backend:install": "cd backend && npm install",
    "install:all": "npm run frontend:install && npm run backend:install",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "keywords": [
    "portfolio",
    "react",
    "nextjs",
    "express",
    "mongodb",
    "fullstack"
  ],
  "author": "Kush Bhardwaj",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  },
  "workspaces": [
    "frontend",
    "backend"
  ]
}
